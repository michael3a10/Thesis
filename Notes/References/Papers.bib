  author = {Yan, Run and McClure, Thomas S. and Jasim, Ibrahem H. and Koppula, Akshay Kumar Reddy and Wang, Sheng and Almasri, Mahmoud and Chen, Chung-Lung},
  date = {2018-11-12},
  journaltitle = {Applied Physics Letters},
