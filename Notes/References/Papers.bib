@article{kimAutomatedDigitalMicrofluidic2011,
  title = {Automated {{Digital Microfluidic Sample Preparation}} for {{Next-Generation DNA Sequencing}}},
  author = {Kim, Hanyoup and Bartsch, Michael S. and Renzi, Ronald F. and He, Jim and Van De Vreugde, James L. and Claudnic, Mark R. and Patel, Kamlesh D.},
