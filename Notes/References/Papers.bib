@article{fellerCeramicMultilayerTechnology2021,
  title = {Ceramic Multilayer Technology as a Platform for Miniaturized Sensor Arrays for Water Analysis},
  author = {Feller, Claudia and Partsch, Uwe},
  date = {2021-03-25},
  journaltitle = {Journal of Sensors and Sensor Systems},
  shortjournal = {J. Sens. Sens. Syst.},
