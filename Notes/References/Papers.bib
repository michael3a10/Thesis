@article{xieRapidBacterialIdentification2025,
  title = {Rapid {{Bacterial Identification Through Multiplexed Nucleic Acid Detection}} on a {{Digital Microfluidic Platform}} for {{Enhanced Clinical Intervention Against Infections}}},
  author = {Xie, Ruibin and Shen, Jienan and Zhou, Lintao and Lu, Lianyu and Sun, Duo and Pei, Yue and Zeng, Lin and Gu, Guoqiang and Wang, Yuye and Yu, Hao and Chen, Yunsheng and Xie, Zhongjian and Yang, Hui},
  date = {2025},
  journaltitle = {ACS Sensors},
  pages = {1--28},
  abstract = {Bacterial infections often result in severe health conditions, including infectious shock, sepsis, and multi-organ dysfunction, posing significant diagnostic and treatment challenges. Therefore, a rapid, multiplexed identification is urgently needed to accurately detect a broad spectrum of bacteria, guiding precise antibiotic prescriptions. Here, we introduce a cost-effective digital microfluidic platform designed for multiplexed detection of bacterial infections, which is referred to as MiND-DMF. This system integrates DNA extraction, RPA amplification, and CRISPR-based bacterial detection, enabling the efficient identification of six infectious bacteria: E. coli, S. aureus, MRSA, P. aeruginosa, K. pneumoniae, and H. influenzae. The entire diagnostic process is completed in just 55 minutes at 37°C and is compatible with human reference genes. MiND-DMF exhibits robust anti-interference capabilities, making it ideal for detecting bacteria in different tissue fluids. In spiked samples, the platform demonstrated a detection limit of 100 CFU/mL, highlighting its great sensitivity and quantification capability. In clinical evaluations, MiND-DMF achieved 100\% sensitivity and 98-100\% specificity compared to the hospital's gold-standard method. The system analyzed 50 samples from various tissue sources, accurately pinpointing the bacteria responsible for infections. This information guides the administration of appropriate antibiotic treatment. Notably, K.pneumoniae and P.aeruginosa are the leading causes of nosocomial infections in ICU, with K.pneumoniae commonly associated with urinary tract infections and E.coli frequently isolated from appendiceal pus. Additionally, H.influenzae primarily affects children under 5-years-old. MiND-DMF’s efficiency and precision support monitoring the origins of hospital-acquired bacterial infections, assisting physicians in prescribing targeted antibiotics and enhancing infection prevention and control strategies.},
  langid = {english},
  keywords = {❓ Multiple DOI},
  file = {F:\OneDrive\PhD\Zotero\Xie et al. - Rapid Bacterial Identification Through Multiplexed Nucleic Acid Detection on a Digital Microfluidic.pdf}
}

