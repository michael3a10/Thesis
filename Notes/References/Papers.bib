@article{pengFingerpoweredElectrophoreticTransport2016,
  title = {Finger-Powered Electrophoretic Transport of Discrete Droplets for Portable Digital Microfluidics},
  author = {Peng, Cheng and Wang, Yide and Sungtaek Ju, Y.},
  date = {2016},
  journaltitle = {Lab on a Chip},
