  annotation = {25 citations (Crossref/DOI) [2025-03-15]},
  file = {F:\OneDrive\PhD\Zotero\Kosri et al. - 2022 - Micro and Nano Interdigitated Electrode Array (IDEA)-Based MEMSNEMS as Electrochemical Transducers.pdf}
