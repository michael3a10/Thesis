  author = {Aakre, Kristin M. and Langlois, Michel R. and Watine, Joseph and Barth, Julian H. and Baum, Hannsjörg and Collinson, Paul and Laitinen, Païvi and Oosterhuis, Wytze P.},
  date = {2013-06-01},
