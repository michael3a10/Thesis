@article{zhongTheoryDielectrophoresisIts2017,
  title = {The Theory of Dielectrophoresis and Its Applications on Medical and Materials Research},
  author = {Zhong, Xiao and Sun, Yuan and Kang, Chen and Wan, Gang},
