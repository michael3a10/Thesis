  annotation = {31 citations (Crossref/DOI) [2025-03-15]},
  file = {F:\OneDrive\PhD\Zotero\Munshi et al. - 2018 - Use of 3D printing and modular microfluidics to integrate cell culture, injections and electrochemic.pdf}
}

