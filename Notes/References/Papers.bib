@inproceedings{ghaniCriticalEvaluationRiver2020,
  title = {A {{Critical Evaluation Of River Management Models In Malaysia}}},
  author = {Ghani, Aminuddin Ab and Weng, Chan Ngai and Roy, Ranjan and Samat, Narimah and Hui, Lai Chee and Leong, Tan Mou},
