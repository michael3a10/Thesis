@article{tsaiMicrosystemAdvancesIntegration2023,
  title = {Microsystem {{Advances}} through {{Integration}} with {{Artificial Intelligence}}},
  author = {Tsai, Hsieh-Fu and Podder, Soumyajit and Chen, Pin-Yuan},
  date = {2023-04-08},
  journaltitle = {Micromachines},
  shortjournal = {Micromachines},
