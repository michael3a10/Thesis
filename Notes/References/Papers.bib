@article{kremersMicroelectrodeCombinationsGold2021,
  title = {Microelectrode {{Combinations}} of {{Gold}} and {{Polypyrrole Enable Highly Stable Two}}‐electrode {{Electrochemical Impedance Spectroscopy Measurements}} under {{Turbulent Flow Conditions}}},
  author = {Kremers, Tom and Tintelott, Marcel and Pachauri, Vivek and Vu, Xuan Thang and Ingebrandt, Sven and Schnakenberg, Uwe},
  date = {2021-01},
  journaltitle = {Electroanalysis},
