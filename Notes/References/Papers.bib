  author = {Kreutz, Jason E. and Munson, Todd and Huynh, Toan and Shen, Feng and Du, Wenbin and Ismagilov, Rustem F.},
  date = {2011-11-01},
  journaltitle = {Analytical Chemistry},
  shortjournal = {Anal. Chem.},
