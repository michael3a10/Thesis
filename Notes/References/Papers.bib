@article{ahmedDesignValidationMicrofluidic2021,
  title = {Design and Validation of Microfluidic Parameters of a Microfluidic Chip Using Fluid Dynamics},
  author = {Ahmed, Feroz and Yoshida, Yuichi and Wang, Jin and Sakai, Kenji and Kiwa, Toshihiko},
  date = {2021-07-01},
  journaltitle = {AIP Advances},
  volume = {11},
