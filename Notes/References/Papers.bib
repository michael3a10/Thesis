@article{farzbodIntegrationReconfigurablePotentiometric2018,
  title = {Integration of Reconfigurable Potentiometric Electrochemical Sensors into a Digital Microfluidic Platform},
  author = {Farzbod, Ali and Moon, Hyejin},
  date = {2018-05},
  journaltitle = {Biosensors and Bioelectronics},
  volume = {106},
