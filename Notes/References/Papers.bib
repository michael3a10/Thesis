@article{brownRealTimeModelingCorrection2020,
  title = {Real-{{Time Modeling}} and {{Correction}} of {{Sensor Inputs}} and {{Outputs}} for {{Increased Accuracy}} and {{Reliability}}},
  author = {Brown, Michael A. and Davis, Brian K. and Miller, Jerin R. and Scheffler, Towner B. and Sexton, R. Kevin},
