@article{dasAcoustothermalHeatingSurface2019,
  title = {Acoustothermal Heating in Surface Acoustic Wave Driven Microchannel Flow},
  author = {Das, Pradipta Kr. and Snider, Arthur D. and Bhethanabotla, Venkat R.},
  date = {2019-10-01},
  journaltitle = {Physics of Fluids},
  volume = {31},
