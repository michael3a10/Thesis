  shorttitle = {Biomedical {{Applications}} of {{Microfluidic Devices}}},
  author = {Gharib, Ghazaleh and Bütün, İsmail and Muganlı, Zülâl and Kozalak, Gül and Namlı, İlayda and Sarraf, Seyedali Seyedmirzaei and Ahmadi, Vahid Ebrahimpour and Toyran, Erçil and Van Wijnen, Andre J. and Koşar, Ali},
