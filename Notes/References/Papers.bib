  author = {Tsaloglou, Maria-Nefeli and Jacobs, Adrian and Morgan, Hywel},
  date = {2014-09},
  journaltitle = {Analytical and Bioanalytical Chemistry},
  shortjournal = {Anal Bioanal Chem},
  volume = {406},
