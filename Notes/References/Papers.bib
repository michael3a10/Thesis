@article{ahmadHighlySensitiveNonenzymatic2017,
  title = {A {{Highly Sensitive Nonenzymatic Sensor Based}} on {{Fe}}{\textsubscript{2}} {{O}}{\textsubscript{3}} {{Nanoparticle Coated ZnO Nanorods}} for {{Electrochemical Detection}} of {{Nitrite}}},
  author = {Ahmad, Rafiq and Ahn, Min‐Sang and Hahn, Yoon‐Bong},
