@article{abdullatipLowcostPolyurethaneCoating2023,
  title = {Low-Cost {{Polyurethane Coating}} as {{Dielectric Component}} in {{Digital Microfluidics}}},
  author = {Abdul Latip, E. N. and Coudron, L. and Tracey, M. C.},
  date = {2023-01-15},
  journaltitle = {Journal of Mechanical Engineering},
  shortjournal = {JMechE},
