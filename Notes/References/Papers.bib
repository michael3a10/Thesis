@article{jeroishMicroheaterMaterialDesign2022,
  title = {Microheater: Material, Design, Fabrication, Temperature Control, and Applications—a Role in {{COVID-19}}},
  shorttitle = {Microheater},
  author = {Jeroish, Z. E. and Bhuvaneshwari, K. S. and Samsuri, Fahmi and Narayanamurthy, Vigneswaran},
