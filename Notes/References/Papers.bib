@article{khanInfluenceGroundElectrode2023,
  title = {Influence of the {{Ground Electrode}} on the {{Dynamics}} of {{Electrowetting}}},
  author = {Khan, Iftekhar and Castelletto, Stefania and Rosengarten, Gary},
  date = {2023-01-30},
  journaltitle = {Micromachines},
  shortjournal = {Micromachines},
  volume = {14},
