@article{cuarteroWhyAmmoniumDetection2020,
  title = {Why Ammonium Detection Is Particularly Challenging but Insightful with Ionophore-Based Potentiometric Sensors – an Overview of the Progress in the Last 20 Years},
  author = {Cuartero, María and Colozza, Noemi and Fernández-Pérez, Bibiana M. and Crespo, Gastón A.},
  date = {2020},
  journaltitle = {The Analyst},
  shortjournal = {Analyst},
