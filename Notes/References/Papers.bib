@article{choElectricalElectrochemicalSensors2021,
  title = {Electrical and {{Electrochemical Sensors Based}} on {{Carbon Nanotubes}} for the {{Monitoring}} of {{Chemicals}} in {{Water}}—{{A Review}}},
  author = {Cho, Gookbin and Azzouzi, Sawsen and Zucchi, Gaël and Lebental, Bérengère},
  date = {2021-12-29},
  journaltitle = {Sensors},
  shortjournal = {Sensors},
  volume = {22},
