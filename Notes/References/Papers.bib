  shorttitle = {Modification of the {{Water Quality Index}} ({{WQI}}) {{Process}} for {{Simple Calculation Using}} the {{Multi-Criteria Decision-Making}} ({{MCDM}}) {{Method}}},
  author = {Akhtar, Naseem and Ishak, Muhammad Izzuddin Syakir and Ahmad, Mardiana Idayu and Umar, Khalid and Md Yusuff, Mohamad Shaiful and Anees, Mohd Talha and Qadir, Abdul and Ali Almanasir, Yazan Khalaf},
