@article{burguillosMicrochannelAcoustophoresisDoes2013,
  title = {Microchannel {{Acoustophoresis}} Does Not {{Impact Survival}} or {{Function}} of {{Microglia}}, {{Leukocytes}} or {{Tumor Cells}}},
  author = {Burguillos, Miguel A. and Magnusson, Cecilia and Nordin, Maria and Lenshof, Andreas and Augustsson, Per and Hansson, Magnus J. and Elmér, Eskil and Lilja, Hans and Brundin, Patrik and Laurell, Thomas and Deierborg, Tomas},
  editor = {Pizzo, Salvatore V.},
  date = {2013-05-27},
