@article{yeDesignFabricationCharacterisation2018,
  title = {Design, Fabrication and Characterisation of {{Si}}‐based Capillary‐driven Microfluidic Devices},
  author = {Ye, Yifei and Zhao, Yang and Cheng, Jie and Li, Mingxiao and Huang, Chengjun},
