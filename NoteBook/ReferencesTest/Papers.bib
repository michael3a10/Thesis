@article{hePhotofabricatedChanneldigitalMicrofluidics2024,
  title = {Photofabricated Channel-Digital Microfluidics ({{pCDMF}}): {{A}} Promising Lab-on-a-Chip Platform for Fully Integrated Digital {{PCR}}},
  shorttitle = {Photofabricated Channel-Digital Microfluidics ({{pCDMF}})},
  author = {He, Yu and Lu, Zefan and Liu, Ke and Wang, Lan and Xu, Qiudi and Fan, Hongliang and Liu, Chong and Zhang, Tao},
  year = {2024},
