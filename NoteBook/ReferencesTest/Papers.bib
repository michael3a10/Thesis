@article{kremersPortaDropPortableDigital2020,
  title = {{{PortaDrop}}: {{A}} Portable Digital Microfluidic Platform Providing Versatile Opportunities for {{Lab-On-A-Chip}} Applications},
  shorttitle = {{{PortaDrop}}},
  author = {Kremers, Tom and Thelen, Sarah and Bosbach, Nils and Schnakenberg, Uwe},
