@article{yangMicrofluidicPointofCarePOC2022,
  title = {Microfluidic {{Point-of-Care}} ({{POC}}) {{Devices}} in {{Early Diagnosis}}: {{A Review}} of {{Opportunities}} and {{Challenges}}},
