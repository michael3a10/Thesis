@article{wangMicrofluidicSamplingBiosensing2022,
  title = {Microfluidic {{Sampling}} and {{Biosensing Systems}} for {{Foodborne}} {{{\emph{Escherichia}}}}{\emph{ Coli}} and {{{\emph{Salmonella}}}}},
  author = {Wang, Bin and Park, Bosoon},
