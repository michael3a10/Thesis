@article{royCongestionawareLayoutDesign2012,
  title = {Congestion-Aware Layout Design for High-Throughput Digital Microfluidic Biochips},
  author = {Roy, Sudip and Mitra, Debasis and Bhattacharya, Bhargab B. and Chakrabarty, Krishnendu},
  year = {2012},
