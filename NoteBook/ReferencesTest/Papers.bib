@article{guanNumericalInvestigationContinuous2020,
  title = {Numerical Investigation of Continuous Droplet Transport in Parallel-Plate Electrowetting-on-Dielectric Digital Microfluidics ({{EWOD DMF}}) with Stripped Electrodes},
  author = {Guan, Yin and Zhu, Mengnan and Li, Baiyun and Tu, Jiyue and Xing, Lu and Chen, Xiyang},
