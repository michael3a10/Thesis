@article{kaminskiControlledDropletMicrofluidic2017,
  title = {Controlled Droplet Microfluidic Systems for Multistep Chemical and Biological Assays},
  author = {Kaminski, T. S. and Garstecki, P.},
  year = {2017},
  journal = {Chemical Society Reviews},
  volume = {46},
  number = {20},
