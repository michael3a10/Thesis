@article{jiaArtificialIntelligenceenabledMultipurpose2024,
  title = {Artificial Intelligence-Enabled Multipurpose Smart Detection in Active-Matrix Electrowetting-on-Dielectric Digital Microfluidics},
  author = {Jia, Zhiqiang and Chang, Chunyu and Hu, Siyi and Li, Jiahao and Ge, Mingfeng and Dong, Wenfei and Ma, Hanbin},
