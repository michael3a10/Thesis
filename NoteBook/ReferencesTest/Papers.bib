@article{ozRapidSensitiveDetection2020,
  title = {Rapid and Sensitive Detection of {{Salmonella}} Spp. in Raw Minced Meat Samples Using Droplet Digital {{PCR}}},
  author = {{\"O}z, Yeliz Y{\"u}cel and S{\"o}nmez, {\"O}yk{\"u} {\.I}rig{\"u}l and Karaman, Sibel and {\"O}z, Ersoy and Unal, Can Bora and Karata{\c s}, Ayten Yazgan},
