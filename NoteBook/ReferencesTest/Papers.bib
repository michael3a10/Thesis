@article{elviraMaterialsMethodsDroplet2022,
  title = {Materials and Methods for Droplet Microfluidic Device Fabrication},
  author = {Elvira, Katherine S. and Gielen, Fabrice and Tsai, Scott S. H. and Nightingale, Adrian M.},
