@article{somvanshiMicrofluidicPaperbasedAptasensor2022,
  title = {Microfluidic Paper-Based Aptasensor Devices for Multiplexed Detection of Pathogenic Bacteria},
  author = {Somvanshi, Sandeep B. and Ulloa, Ana M. and Zhao, Min and Liang, Qiyue and Barui, Amit Kumar and Lucas, Alec and Jadhav, K.M. and Allebach, Jan P. and Stanciu, Lia A.},
  year = {2022},
