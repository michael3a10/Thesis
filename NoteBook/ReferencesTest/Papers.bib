@article{sistaDigitalMicrofluidicPlatform2020,
  title = {Digital {{Microfluidic Platform}} to {{Maximize Diagnostic Tests}} with {{Low Sample Volumes}} from {{Newborns}} and {{Pediatric Patients}}},
  author = {Sista, Rama S. and Ng, Rainer and Nuffer, Miriam and Basmajian, Michael and Coyne, Jacob and Elderbroom, Jennifer and Hull, Daniel and Kay, Kathryn and Krishnamurthy, Maithri and Roberts, Christopher and Wu, Daniel and Kennedy, Adam D. and Singh, Rajendra and Srinivasan, Vijay and Pamula, Vamsee K.},
  year = {2020},
  month = jan,
  journal = {Diagnostics},
  volume = {10},
  number = {1},
  pages = {21},
  issn = {2075-4418},
  doi = {10.3390/diagnostics10010021},
  urldate = {2024-01-25},
  abstract = {Children are not tiny adults'' is an adage commonly used in pediatrics to emphasize the fact that children often have different physiological responses to sickness and trauma compared to adults. However, despite widespread acceptance of this concept, diagnostic blood testing is an excellent example of clinical care that is not yet customized to the needs of children, especially newborns. Cumulative blood loss resulting from clinical testing does not typically impact critically ill adult patients, but can quickly escalate in children, leading to iatrogenic anemia and related comorbidities. Moreover, the tests prioritized for rapid, near-patient testing in adults are not always the most clinically relevant tests for children or newborns. This report describes the development of a digital microfluidic testing platform and associated clinical assays purposely curated to address current shortcomings in pediatric laboratory testing by using microliter volumes ({$<$}50 {\textmu}L) of samples. The automated platform consists of a small instrument and single-use cartridges, which contain all reagents necessary to prepare the sample and perform the assay. Electrowetting technology is used to precisely manipulate nanoliter-sized droplets of samples and reagents inside the cartridge. To date, we have automated three disparate types of assays (biochemical assays, immunoassays, and molecular assays) on the platform and have developed over two dozen unique tests, each with important clinical application to newborns and pediatric patients. Cell lysis, plasma preparation, magnetic bead washing, thermocycling, incubation, and many other essential functions were all performed on the cartridge without any user intervention. The resulting assays demonstrate performance comparable to standard clinical laboratory assays and are economical due to the reduced hands-on effort required for each assay and lower overall reagent consumption. These capabilities allow a wide range of assays to be run simultaneously on the same cartridge using significantly reduced sample volumes with results in minutes.},
  langid = {english},
  annotation = {36 citations (Crossref/DOI) [2024-11-14]},
  file = {C:\Users\micha\OneDrive\PhD\Zotero_MSI\Sista et al. - 2020 - Digital Microfluidic Platform to Maximize Diagnostic Tests with Low Sample Volumes from Newborns and.pdf}
}

