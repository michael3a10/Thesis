@article{haleElectrowettingbasedMicrofluidicOperations2017,
  title = {Electrowetting-Based Microfluidic Operations on Rapid-Manufactured Devices for Heat Pipe Applications},
  author = {Hale, Renee S and Bahadur, Vaibhav},
  year = {2017},
  month = jul,
  journal = {Journal of Micromechanics and Microengineering},
  volume = {27},
