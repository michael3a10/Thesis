@article{samieiIntegrationBiosensorsDigital2016,
  title = {Integration of Biosensors into Digital Microfluidics: {{Impact}} of Hydrophilic Surface of Biosensors on Droplet Manipulation},
  shorttitle = {Integration of Biosensors into Digital Microfluidics},
  author = {Samiei, Ehsan and Luka, George S. and Najjaran, Homayoun and Hoorfar, Mina},
  year = {2016},
