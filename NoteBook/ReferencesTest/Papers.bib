@article{mostafaRaspberryPiDesign2022,
  title = {Raspberry {{Pi Design}} and {{Hardware Implementation}} of {{Fuzzy-PI Controller}} for {{Three-Phase Grid-Connected Inverter}}},
  author = {Mostafa, Sameh and Zekry, Abdelhalim and Youssef, Ayman and Anis, Wagdi Refaat},
