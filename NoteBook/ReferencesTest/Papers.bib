@article{luFlexibleDropletRouting2018,
  title = {Flexible {{Droplet Routing}} in {{Active Matrix}}--{{Based Digital Microfluidic Biochips}}},
  author = {Lu, Guan-Ruei and Kuo, Chun-Hao and Chiang, Kuen-Cheng and Banerjee, Ansuman and Bhattacharya, Bhargab B. and Ho, Tsung-Yi and Chen, Hung-Ming},
