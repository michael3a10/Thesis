@article{wuLiquidDielectrophoresisElectret2012,
  title = {Liquid {{Dielectrophoresis}} on {{Electret}}: {{A Novel Approach Towards CMOS-Driven Digital Microfludics}}},
  shorttitle = {Liquid {{Dielectrophoresis}} on {{Electret}}},
