@article{bhargavaDiscreteElements3D2014,
  title = {Discrete Elements for {{3D}} Microfluidics},
  author = {Bhargava, Krisna C. and Thompson, Bryant and Malmstadt, Noah},
  year = {2014},
  month = oct,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {111},
