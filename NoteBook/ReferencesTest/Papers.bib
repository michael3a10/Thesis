@article{choiDropletTransportationUsing2012,
  title = {Droplet Transportation Using a Pre-Charging Method for Digital Microfluidics},
  author = {Choi, Kyungyong and Im, Maesoon and Choi, Ji-Min and Choi, Yang-Kyu},
  year = {2012},
  month = mar,
