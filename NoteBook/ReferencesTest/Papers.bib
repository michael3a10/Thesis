}

@article{hanDigitalQuantificationSelection2019,
  title = {Digital Quantification and Selection of High-Lipid-Producing Microalgae through a Lateral Dielectrophoresis-Based Microfluidic Platform},
  author = {Han, Song-I and Kim, Hyun Soo and Han, Ki-Ho and Han, Arum},
  year = {2019},
