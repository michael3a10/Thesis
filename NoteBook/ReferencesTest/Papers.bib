@article{zhangDropCarbaAutomatedMagnetic2023,
  title = {{{DropCarba}} -- {{An}} Automated Magnetic Digital Microfluidic Platform for Rapid Phenotypic Testing of Carbapenemase-Producing {{Gram-negative}} Bacilli},
  author = {Zhang, Yi and Yazid, Nurhidayah Binte Mohamed and Ho, Pei-Yun and Hu, Xuyang and Chen, Songlin and Vasoo, Shawn and Kanitthamniyom, Pojchanun},
