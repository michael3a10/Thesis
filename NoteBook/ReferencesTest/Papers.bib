@article{longPiRamidCompactRaspberry2022,
  title = {{{PiRamid}}: {{A}} Compact {{Raspberry Pi}} Imaging Box to Automate Small-Scale Time-Lapse Digital Analysis, Suitable for Laboratory and Field Use},
