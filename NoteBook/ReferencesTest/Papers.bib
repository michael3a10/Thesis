@inproceedings{bhattacharjeeEffectsPropertiesDielectric2010,
  title = {Effects of the {{Properties}} of {{Dielectric Materials}} on the {{Fabrication}} and {{Operation}} of {{Digital Microfluidic Systems}}},
  booktitle = {Volume 10: {{Micro}} and {{Nano Systems}}},
  author = {Bhattacharjee, Biddut and Najjaran, Homayoun},
  year = {2010},
