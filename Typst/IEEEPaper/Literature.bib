@article{huangEfficientAlgorithmOptimizing2020,
  title = {An {{Efficient Algorithm}} for {{Optimizing}} the {{Test Path}} of {{Digital Microfluidic Biochips}}},
  author = {Huang, Xijun and Xu, Chuanpei and Zhang, Long},
  year = {2020},
  month = apr,
  journal = {Journal of Electronic Testing},
  volume = {36},
