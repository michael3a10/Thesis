@article{leeAutomated3DprintedSmartphone2019,
  title = {An Automated {{3D-printed}} Smartphone Platform Integrated with Optoelectrowetting ({{OEW}}) Microfluidic Chip for on-Site Monitoring of Viable Algae in Water},
