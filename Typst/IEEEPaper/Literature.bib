@article{abdelgawadOptimizationDeviceGeometry2009,
  title = {Optimization of Device Geometry in Single-Plate Digital Microfluidics},
  author = {Abdelgawad, Mohamed and Park, Philip and Wheeler, Aaron R.},
  year = {2009},
