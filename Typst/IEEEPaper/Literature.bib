  author = {Roy, Sudip and Mitra, Debasis and Bhattacharya, Bhargab B. and Chakrabarty, Krishnendu},
  year = {2012},
  month = aug,
  journal = {ACM Journal on Emerging Technologies in Computing Systems},
  volume = {8},
