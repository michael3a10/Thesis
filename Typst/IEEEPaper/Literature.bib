  author = {Hua, Zhishan and Rouse, Jeremy L. and Eckhardt, Allen E. and Srinivasan, Vijay and Pamula, Vamsee K. and Schell, Wiley A. and Benton, Jonathan L. and Mitchell, Thomas G. and Pollack, Michael G.},
