@article{huangDesignCriteriaApplications2018,
  title = {Design Criteria and Applications of Multi-Channel Parallel Microfluidic Module},
  author = {Huang, Yichao and Han, Tengteng and Xuan, Jin and Xu, Hong and Wang, Yongle and Zhang, Li},
  year = {2018},
  month = oct,
  journal = {Journal of Micromechanics and Microengineering},
