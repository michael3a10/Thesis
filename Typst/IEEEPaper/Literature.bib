@article{wenDigitalPlatformOnePot2024,
  title = {A Digital Platform for {{One-Pot}} Signal Enhanced Foodborne Pathogen Detection Based on Mesophilic Argonaute-Driven Polydisperse Microdroplet Reactors and Machine Learning},
