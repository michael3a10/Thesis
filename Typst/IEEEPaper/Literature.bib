@article{ghoshContinuousActuationLiquid2025,
  title = {The Continuous Actuation of Liquid Metal with a {{3D-printed}} Electrowetting Device},
  author = {Ghosh, Samannoy and Neupane, Rajan and Sahu, Dwipak Prasad and Teng, Jian and Kong, Yong Lin},
