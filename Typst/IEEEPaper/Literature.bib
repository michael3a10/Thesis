@article{kanitthamniyomMagneticDigitalMicrofluidics2019,
  title = {Magnetic Digital Microfluidics on a Bioinspired Surface for Point-of-care Diagnostics of Infectious Disease},
  author = {Kanitthamniyom, Pojchanun and Zhang, Yi},
  year = {2019},
  month = apr,
  journal = {ELECTROPHORESIS},
  volume = {40},
