@article{hsuBeadNumberEffect2022,
  title = {Bead {{Number Effect}} in a {{Magnetic-Beads-Based Digital Microfluidic Immunoassay}}},
  author = {Hsu, Wensyang and Shih, Yu-Teng and Lee, Meng-Shiue and Huang, Hong-Yuan and Wu, Wan-Ning},
  year = {2022},
  month = may,
  journal = {Biosensors},
  volume = {12},
