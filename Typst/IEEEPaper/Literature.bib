  author = {Moore, J. A. and {Nemat-Gorgani}, M. and Madison, A. C. and Sandahl, M. A. and Punnamaraju, S. and Eckhardt, A. E. and Pollack, M. G. and Vigneault, F. and Church, G. M. and Fair, R. B. and Horowitz, M. A. and Griffin, P. B.},
