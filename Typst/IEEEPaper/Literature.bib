@article{thakurLowCostEaseofAccess2022,
  title = {Low {{Cost}}, {{Ease-of-Access Fabrication}} of {{Microfluidic Devices Using Wet Paper Molds}}},
  author = {Thakur, Raviraj and Fridman, Gene Y.},
