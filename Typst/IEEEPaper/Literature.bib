  shorttitle = {Towards an {{Integrated Approach}} to {{Improve}} the {{Understanding}} of the {{Relationships Between Water-Borne Infections}} and {{Health Outcomes}}},
  author = {Ho, Jia Yee and Lavinya, Amanda Anne and Kay, Dominic Shuen Wei and Lee, Cindy Ik Sing and Razmi, Ahmad Haikal and Walsh, Claire L. and Goodson, Michaela L. and Eswaran, Jeyanthy},
