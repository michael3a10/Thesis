@article{sadeghianInterdigitatedElectrodeDesign2017,
  title = {Interdigitated Electrode Design and Optimization for Dielectrophoresis Cell Separation Actuators},
  author = {Sadeghian, Hesam and Hojjat, Yousef and Soleimani, Masoud},
  year = {2017},
