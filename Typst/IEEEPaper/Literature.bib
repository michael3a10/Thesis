@article{strohmeierCentrifugalMicrofluidicPlatforms2015,
  title = {Centrifugal Microfluidic Platforms: Advanced Unit Operations and Applications},
  shorttitle = {Centrifugal Microfluidic Platforms},
  author = {Strohmeier, O. and Keller, M. and Schwemmer, F. and Zehnle, S. and Mark, D. and Von Stetten, F. and Zengerle, R. and Paust, N.},
