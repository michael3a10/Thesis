@article{climeNumericalModelingElectrowetting2010,
  title = {Numerical Modeling of Electrowetting Transport Processes for Digital Microfluidics},
  author = {Clime, L. and Brassard, D. and Veres, T.},
  year = {2010},
  month = may,
  journal = {Microfluidics and Nanofluidics},
