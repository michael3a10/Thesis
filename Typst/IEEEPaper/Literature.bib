}

@article{pacholewiczEnvironmentalSamplingMethods2023,
  title = {Environmental {{Sampling Methods}} for {{Detection}} of {{Salmonella Infections}} in {{Laying Hens}}: {{A Systematic Review}} and {{Meta-Analysis}}},
  shorttitle = {Environmental {{Sampling Methods}} for {{Detection}} of {{Salmonella Infections}} in {{Laying Hens}}},
