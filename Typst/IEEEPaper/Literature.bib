@article{sutterSolidcontactPolymericMembrane2004,
  title = {Solid-Contact Polymeric Membrane Electrodes with Detection Limits in the Subnanomolar Range},
  author = {Sutter, Jolanda and Radu, Aleksandar and Peper, Shane and Bakker, Eric and Pretsch, Ern{\"o}},
