@article{mashraeiIntegrationFractalBiosensor2016,
  title = {Integration of {{Fractal Biosensor}} in a {{Digital Microfluidic Platform}}},
  author = {Mashraei, Yousof and Sivashankar, Shilpa and Buttner, Ulrich and Salama, Khaled Nabil},
  year = {2016},
