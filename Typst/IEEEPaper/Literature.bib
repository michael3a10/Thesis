@article{ngDigitalMicrofluidicPlatform2015,
  title = {Digital {{Microfluidic Platform}} for the {{Detection}} of {{Rubella Infection}} and {{Immunity}}: {{A Proof}} of {{Concept}}},
  shorttitle = {Digital {{Microfluidic Platform}} for the {{Detection}} of {{Rubella Infection}} and {{Immunity}}},
  author = {Ng, Alphonsus H C and Lee, Misan and Choi, Kihwan and Fischer, Andrew T and Robinson, John M and Wheeler, Aaron R},
