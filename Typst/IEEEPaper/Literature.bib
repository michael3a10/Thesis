  author = {Zhao, Qing and Tang, Biao and Dong, Baoqin and Li, Hui and Zhou, Rui and Guo, Yuanyuan and Dou, Yingying and Deng, Yong and Groenewold, Jan and Henzen, Alexander Victor and Zhou, Guofu},
