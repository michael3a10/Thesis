@article{guanDeformationSpeedStability2019,
  title = {Deformation, Speed, and Stability of Droplet Motion in Closed Electrowetting-Based Digital Microfluidics},
  author = {Guan, Yin and Li, Baiyun and Zhu, Mengnan and Cheng, Shengjie and Tu, Jiyue},
