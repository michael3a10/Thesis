@article{papadopoulouEfficientPurePolyimide2016,
  title = {An Efficient Pure Polyimide Ammonia Sensor},
  author = {Papadopoulou, Evie L. and Morselli, Davide and Prato, Mirko and Barcellona, Alessandro and Athanassiou, Athanassia and Bayer, Ilker S.},
