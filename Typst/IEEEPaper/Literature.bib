@article{strachanSimpleIntegratedMicrofluidic2016,
  title = {A Simple Integrated Microfluidic Device for the Multiplexed Fluorescence-Free Detection of {{Salmonella}} Enterica},
  author = {Strachan, Briony C. and Sloane, Hillary S. and Houpt, Eric and Lee, Jacob C. and Miranian, Daniel C. and Li, Jingyi and Nelson, Daniel A. and Landers, James P.},
