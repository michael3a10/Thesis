  author = {Coelho, Beatriz and Neto, Joana and Sieira, B{\'a}rbara and Moura, Andr{\'e} and Fortunato, Elvira and Martins, Rodrigo and Baptista, Pedro and Igreja, Rui and {\'A}guas, Hugo},
