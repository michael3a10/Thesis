  author = {Kurniawan, Yehezkiel Steven and Imawan, Arif Cahyo and Rao, Sathuluri Ramachandra and Ohto, Keisuke and Iwasaki, Wataru and Miyazaki, Masaya and {Jumina}},
  year = {2019},
  month = aug,
