@article{drydenDStatVersatileOpenSource2015,
  title = {{{DStat}}: {{A Versatile}}, {{Open-Source Potentiostat}} for {{Electroanalysis}} and {{Integration}}},
  shorttitle = {{{DStat}}},
  author = {Dryden, Michael D. M. and Wheeler, Aaron R.},
  editor = {Eddington, David T.},
  year = {2015},
