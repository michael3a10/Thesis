@article{swyerDigitalMicrofluidicsNuclear2019,
  title = {Digital Microfluidics and Nuclear Magnetic Resonance Spectroscopy for {\emph{in Situ}} Diffusion Measurements and Reaction Monitoring},
  author = {Swyer, Ian and Von Der Ecken, Sebastian and Wu, Bing and Jenne, Amy and Soong, Ronald and Vincent, Franck and Schmidig, Daniel and Frei, Thomas and Busse, Falko and Stronks, Henry J. and Simpson, Andr{\'e} J. and Wheeler, Aaron R.},
  year = {2019},
  journal = {Lab on a Chip},
  volume = {19},
  number = {4},
  pages = {641--653},
  issn = {1473-0197, 1473-0189},
  doi = {10.1039/C8LC01214H},
  urldate = {2025-06-14},
  abstract = {We describe a two-plate digital microfluidic method for interfacing with nuclear magnetic resonance spectroscopy (DMF-NMR) for microscale chemical analysis.           ,                             In recent years microcoils and related structures have been developed to increase the mass sensitivity of nuclear magnetic resonance spectroscopy, allowing this extremely powerful analytical technique to be extended to small sample volumes ({$<$}5 {$\mu$}l). In general, microchannels have been used to deliver the samples of interest to these microcoils; however, these systems tend to have large dead volumes and require more complex fluidic connections. Here, we introduce a two-plate digital microfluidic (DMF) strategy to interface small-volume samples with NMR microcoils. In this system, a planar microcoil is surrounded by a copper plane that serves as the counter-electrode for the digital microfluidic device, allowing for precise control of droplet position and shape. This feature allows for the user-determination of the orientation of droplets relative to the main axes of the shim stack, permitting improved shimming and a more homogeneous magnetic field inside the droplet below the microcoil, which leads to improved spectral lineshape. This, along with high-fidelity droplet actuation, allows for rapid shimming strategies (developed over decades for vertically oriented NMR tubes) to be employed, permitting the determination of reaction-product diffusion coefficients as well as quantitative monitoring of reactive intermediates. We propose that this system paves the way for new and exciting applications for               in situ               analysis of small samples by NMR spectroscopy.},
  langid = {english},
  keywords = {ccfInfo: Not Found,citationNumber: 38},
  annotation = {43 citations (Crossref/DOI) [2025-07-01]\\
