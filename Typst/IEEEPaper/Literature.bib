@article{gabayShapingLiquidFilms2021,
  title = {Shaping Liquid Films by Dielectrophoresis},
  author = {Gabay, Israel and Paratore, Federico and Boyko, Evgeniy and Ramos, Antonio and Gat, Amir D. and Bercovici, Moran},
