@article{joshiReviewChemiresistiveRoom2018,
  title = {A Review on Chemiresistive Room Temperature Gas Sensors Based on Metal Oxide Nanostructures, Graphene and {{2D}} Transition Metal Dichalcogenides},
  author = {Joshi, Nirav and Hayasaka, Takeshi and Liu, Yumeng and Liu, Huiliang and Oliveira, Osvaldo N. and Lin, Liwei},
  year = {2018},
