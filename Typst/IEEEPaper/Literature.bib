@article{baraniNumericalExperimentalAnalysis2021,
  title = {Numerical and Experimental Analysis of a Hybrid Material Acoustophoretic Device for Manipulation of Microparticles},
  author = {Barani, Alireza and Mosaddegh, Peiman and Haghjooy Javanmard, Shaghayegh and Sepehrirahnama, Shahrokh and {Sanati-Nezhad}, Amir},
