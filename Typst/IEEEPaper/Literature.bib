@article{huangDigitalMicrofluidicRTqPCR2022,
  title = {A {{Digital Microfluidic RT-qPCR Platform}} for {{Multiple Detections}} of {{Respiratory Pathogens}}},
  author = {Huang, Huitao and Huang, Kaisong and Sun, Yun and Luo, Dasheng and Wang, Min and Chen, Tianlan and Li, Mingzhong and Duan, Junwei and Huang, Liqun and Dong, Cheng},
