@article{huangDigitalMicrofluidicsDetection2020,
  title = {Digital {{Microfluidics}} for the {{Detection}} of {{Selected Inorganic Ions}} in {{Aerosols}}},
  author = {Huang, Shuquan and Connolly, Jessica and Khlystov, Andrei and Fair, Richard B.},
  year = {2020},
