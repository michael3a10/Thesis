@article{parkIntegratedPumplessMicrofluidic2021,
  title = {Integrated Pumpless Microfluidic Chip for the Detection of Foodborne Pathogens by Polymerase Chain Reaction and Electrochemical Analysis},
  author = {Park, Yoo Min and Park, Juhwan and Lim, Sun Young and Kwon, Yeji and Bae, Nam Ho and Park, Je-Kyun and Lee, Seok Jae},
