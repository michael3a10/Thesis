@article{chaeOptimumThicknessHydrophobic2014,
  title = {Optimum Thickness of Hydrophobic Layer for Operating Voltage Reduction in {{EWOD}} Systems},
  author = {Chae, Jeong Byung and Kwon, Jun O and Yang, Ji Sun and Kim, Dohyun and Rhee, Kyehan and Chung, Sang Kug},
  year = {2014},
