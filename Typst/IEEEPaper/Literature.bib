@article{haoMicrofluidicBiosensorBased2020,
  title = {A {{Microfluidic Biosensor Based}} on {{Magnetic Nanoparticle Separation}}, {{Quantum Dots Labeling}} and {{MnO2 Nanoflower Amplification}} for {{Rapid}} and {{Sensitive Detection}} of {{Salmonella Typhimurium}}},
