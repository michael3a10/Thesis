@article{ngDigitalMicrofluidicSystem2018,
  title = {A Digital Microfluidic System for Serological Immunoassays in Remote Settings},
  author = {Ng, Alphonsus H. C. and Fobel, Ryan and Fobel, Christian and Lamanna, Julian and Rackus, Darius G. and Summers, Aimee and Dixon, Christopher and Dryden, Michael D. M. and Lam, Charis and Ho, Man and Mufti, Nooman S. and Lee, Victor and Asri, Mohd Afiq Mohd and Sykes, Edward A. and Chamberlain, M. Dean and Joseph, Rachael and Ope, Maurice and Scobie, Heather M. and Knipes, Alaine and Rota, Paul A. and Marano, Nina and Chege, Paul M. and Njuguna, Mary and Nzunza, Rosemary and Kisangau, Ngina and Kiogora, John and Karuingi, Michael and Burton, John Wagacha and Borus, Peter and Lam, Eugene and Wheeler, Aaron R.},
  year = {2018},
