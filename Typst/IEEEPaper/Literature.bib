  title = {Image-Based Feedback and Analysis System for Digital Microfluidics},
  author = {Vo, Philippe Q. N. and Husser, Mathieu C. and Ahmadi, Fatemeh and Sinha, Hugo and Shih, Steve C. C.},
  year = {2017},
