@article{ngDigitalMicrofluidicMagnetic2012,
  title = {Digital {{Microfluidic Magnetic Separation}} for {{Particle-Based Immunoassays}}},
  author = {Ng, Alphonsus H. C. and Choi, Kihwan and Luoma, Robert P. and Robinson, John M. and Wheeler, Aaron R.},
