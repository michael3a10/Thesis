@article{pethigReviewArticleDielectrophoresis2010,
  title = {Review {{Article}}---{{Dielectrophoresis}}: {{Status}} of the Theory, Technology, and Applications},
  shorttitle = {Review {{Article}}---{{Dielectrophoresis}}},
  author = {Pethig, Ronald},
  year = {2010},
  month = jun,
  journal = {Biomicrofluidics},
