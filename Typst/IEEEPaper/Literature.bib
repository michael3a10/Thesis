@article{yiannacouControlledManipulationActive2021,
  title = {Controlled {{Manipulation}} and {{Active Sorting}} of {{Particles Inside Microfluidic Chips Using Bulk Acoustic Waves}} and {{Machine Learning}}},
  author = {Yiannacou, Kyriacos and Sariola, Veikko},
  year = {2021},
