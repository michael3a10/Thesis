@article{huangFundamentalsManipulationBare2023,
  title = {Fundamentals and {{Manipulation}} of {{Bare Droplets}} and {{Liquid Marbles}} as {{Open Microfluidic Platforms}}},
  author = {Huang, Zheng and Xie, Yuanhao and Chen, Huaying and Yu, Zhihang and Shi, Liuyong and Jin, Jing},
