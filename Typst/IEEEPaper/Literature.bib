  title = {Auto-Affitech: An Automated Ligand Binding Affinity Evaluation Platform Using Digital Microfluidics with a Bidirectional Magnetic Separation Method},
  shorttitle = {Auto-Affitech},
  author = {Guo, Jingjing and Lin, Li and Zhao, Kaifeng and Song, Yanling and Huang, Mengjiao and Zhu, Zhi and Zhou, Leiji and Yang, Chaoyong},
  year = {2020},
  journal = {Lab on a Chip},
