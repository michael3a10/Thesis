  title = {Dielectrophoresis Microjets: A Merging of Electromagnetics and Microfluidics for on-Chip Technologies},
  shorttitle = {Dielectrophoresis Microjets},
  booktitle = {{{SPIE Photonics Europe}}},
  author = {Hill, Kyle A. and Collier, Christopher M. and Holzman, Jonathan F.},
