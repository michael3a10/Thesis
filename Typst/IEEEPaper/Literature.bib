  title = {Reversible Electrowetting on Silanized Silicon Nitride},
  author = {Cahill, Brian P. and Giannitsis, Athanasios T. and Land, Raul and Gastrock, Gunter and Pliquett, Uwe and Frense, Dieter and Min, Mart and Beckmann, Dieter},
