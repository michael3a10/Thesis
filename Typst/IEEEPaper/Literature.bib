@article{zhangFundamentalsApplicationsInertial2016,
  title = {Fundamentals and Applications of Inertial Microfluidics: A Review},
  shorttitle = {Fundamentals and Applications of Inertial Microfluidics},
  author = {Zhang, Jun and Yan, Sheng and Yuan, Dan and Alici, Gursel and Nguyen, Nam-Trung and Ebrahimi Warkiani, Majid and Li, Weihua},
  year = {2016},
  journal = {Lab on a Chip},
  volume = {16},
