@article{leeACEWODinducedAsymmetric2022,
  title = {{{AC EWOD-induced}} Asymmetric Droplet Oscillation and Manipulation},
  author = {Lee, Seungmin and Lee, Daeyoung and Choi, Minsuk and Chung, Sang Kug},
  year = {2022},
