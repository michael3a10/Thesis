@article{deshpandeFiniteElementMethod2022,
  title = {Finite Element Method Based Modelling and Analysis of Serpentine Electrodes for Biochemical Sensing},
  author = {Deshpande, Shreyas and Bhand, Sunil and Bacher, Gautam},
