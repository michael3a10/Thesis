@article{xieAllinOnePlatformOnSite2024,
  title = {An {{All-in-One Platform}} for {{On-Site Multiplex Foodborne Pathogen Detection Based}} on {{Channel-Digital Hybrid Microfluidics}}},
  author = {Xie, Mei and Chen, Tianlan and Cai, Zongwei and Lei, Bo and Dong, Cheng},
  year = {2024},
  month = jan,
  journal = {Biosensors},
  volume = {14},
