@article{guoArtificialIntelligenceassistedDigital2024,
  title = {An Artificial Intelligence-Assisted Digital Microfluidic System for Multistate Droplet Control},
  author = {Guo, Kunlun and Song, Zerui and Zhou, Jiale and Shen, Bin and Yan, Bingyong and Gu, Zhen and Wang, Huifeng},
