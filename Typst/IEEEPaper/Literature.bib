@article{coelhoParyleneMultipurposeMaterial2023,
  title = {Parylene {{C}} as a {{Multipurpose Material}} for {{Electronics}} and {{Microfluidics}}},
  author = {Coelho, Beatriz J. and Pinto, Joana V. and Martins, Jorge and Rovisco, Ana and Barquinha, Pedro and Fortunato, Elvira and Baptista, Pedro V. and Martins, Rodrigo and Igreja, Rui},
