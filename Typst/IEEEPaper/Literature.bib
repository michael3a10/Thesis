@article{liuDropletAcoustofluidicPlatform2021,
  title = {A Droplet Acoustofluidic Platform for Time-Controlled Microbead-Based Reactions},
  author = {Liu, Zhenhua and Fornell, Anna and Tenje, Maria},
  year = {2021},
  month = may,
  journal = {Biomicrofluidics},
  volume = {15},
